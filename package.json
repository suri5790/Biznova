    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "mern",
    "business",
    "ai",
    "msme",
    "assistant",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "BizNova Team",
  "license": "Private",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "multer": "^2.0.2"
  }
}
