{
  "name": "biznova",
  "version": "1.0.0",
  "description": "AI-Powered MSME Business Assistant - MERN Stack Application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
